---
import {Icon} from "astro-icon/components";

const  {isClosed} = Astro.props;
---
<div class="topline__toggle xl:hidden" data-aw-toggle-menu >
  <div class="h-full flex items-center justify-center">
    {isClosed &&
      <Icon name="close"/>}
    {!isClosed &&
      <Icon name="menu"/>}
  </div>
</div>
